{
  "workbench.editor.editorActionsLocation": "titleBar",
  "breadcrumbs.enabled": false,
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "editor.padding.top": 5,
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "workbench.editor.showTabs": "single",
  "workbench.activityBar.location": "top",
  "workbench.editor.enablePreview": false,
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "workbench.panel.showLabels": false,
  "powershell.startAsLoginShell.osx": false,
  "powershell.integratedConsole.focusConsoleOnExecute": false,
  "powershell.integratedConsole.showOnStartup": false,
  "workbench.startupEditor": "none",
  "editor.stickyScroll.enabled": false,
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "catppuccin-mocha",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.indentSize": "tabSize",
  "editor.formatOnSave": true,
  "powershell.codeFormatting.avoidSemicolonsAsLineTerminators": true,
  "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
  "powershell.codeFormatting.useConstantStrings": true,
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.codeFormatting.whitespaceBetweenParameters": true,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.startupPrompt": "always",
  "security.workspace.trust.enabled": false,
  "powershell.enableProfileLoading": false,
  "json.schemaDownload.trustedDomains": {
    "https://schemastore.azurewebsites.net/": true,
    "https://raw.githubusercontent.com/": true,
    "https://www.schemastore.org/": true,
    "https://json.schemastore.org/": true,
    "https://json-schema.org/": true,
    "https://esm.sh": true
  },
  "powershell.buttons.showRunButtons": false,
  "debug.openDebug": "neverOpen",
  "editor.tabCompletion": "on",
  "files.defaultLanguage": "powershell",
  "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
  "powershell.buttons.showPanelMovementButtons": true,
  "powershell.codeFolding.showLastLine": false,
  "powershell.sideBar.CommandExplorerVisibility": true,
  "git.openRepositoryInParentFolders": "never"
