{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": " {{ .Icon }} ",
          "foreground": "#363a4f",
          "background": "#8aadf4",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07c",
            "folder_separator_icon": " ‚ùØ ",
            "home_icon": "\uf7db",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#ffe9df",
          "powerline_symbol": "\ue0b0",
          "background": "#363a4f",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#24273a",
          "powerline_symbol": "\ue0b0",
          "background": "#eed49f",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }}",
          "foreground": "#363a4f",
          "background": "#f5c2e7",
          "type": "exit",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#FFE873",
          "background": "#306998",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "template": " \ue718 node {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#1f1e2e",
          "powerline_symbol": "\ue0b6",
          "background": "#a6e3a1",
          "type": "node",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ .FormattedMs }}",
          "foreground": "#cad3f5",
          "powerline_symbol": "\ue0b6",
          "background": "#313244",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "\ue38a",
          "foreground": "#cad3f5",
          "background": "#313244",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\uf061 ",
          "foreground": "#e0f8ff",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
