{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "background": "#8aadf4",
          "foreground": "#363a4f",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ .Icon }} ",
          "type": "os"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffe9df",
          "background": "#363a4f",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf07c",
            "folder_separator_icon": " \u276f ",
            "home_icon": "\uf7db",
            "max_depth": 3,
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "background": "#eed49f",
          "foreground": "#24273a",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_worktree_count": true
          }
        },
        {
          "background": "#f5c2e7",
          "foreground": "#363a4f",
          "background_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ],
          "properties": {
            "style": "roundrock",
            "threshold": 0,
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "exit"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#FFE873",
          "background": "#306998",
          "leading_diamond": " \ue0b6",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "background": "#a6e3a1",
          "foreground": "#1f1e2e",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b6",
          "template": " \ue718 node {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "type": "node",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true,
          "background": "#313244",
          "foreground": "#cad3f5",
          "powerline_symbol": "\ue0b6",
          "template": " {{ .FormattedMs }}",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "background": "#313244",
          "foreground": "#cad3f5",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": "\ue38a",
          "type": "text"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#e0f8ff",
          "foreground_templates": ["{{ if gt .Code 0 }}#f38ba8{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\uf061 ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "osc99": true
}